		<Mob Name="Kunzen Ravager" Entry="59119" />
	</AvoidMobs>

<QuestOrder>

<!-- Farmer Fung Quests -->

	<!-- Fung's Vote I: Yak Attack -->
	<If Condition="HasQuest(30517)">
		<TurnIn QuestName="Farmer Fung's Vote I: Yak Attack" QuestId="30517" TurnInName="Farmer Fung" TurnInId="57298" />
		<PickUp QuestName="Farmer Fung's Vote II: On The Loose" QuestId="30518" GiverName="Farmer Fung" GiverId="57298" />
	</If>

	<!-- Fung's Vote II: On The Loose -->
	<If Condition="HasQuest(30518) &amp;&amp; !IsQuestCompleted(30518)" >
		<CustomBehavior File="Message" Text="Starting On The Loose" LogColor="Green" />
		<CustomBehavior File="FlyTo" DestName="Finding Yaks" X="125.1642" Y="700.6128" Z="151.451" />
		<While Condition="!IsQuestCompleted(30518)" >
			<CustomBehavior File="UserSettings" UseMount="True" />
			<If Condition="Random(3) == 1" >
				<CustomBehavior File="FlyTo" DestName="Finding Yaks" X="125.16" Y="700.61" Z="151.45" />
			<ElseIf Condition="Random(2) == 1" >
				<CustomBehavior File="FlyTo" DestName="Finding Yaks" X="115.79" Y="734.87" Z="151.73" />
			</ElseIf>
			<Else>
				<CustomBehavior File="FlyTo" DestName="Finding Yaks" X="92.48" Y="647.47" Z="154.04" />
			</Else>
			</If>
			<CustomBehavior File="InteractWith" MobId="59491" CollectionDistance="300" NumOfTimes="1" WaitTime="500"/>
			<While Condition="Me.IsOnTransport" >
				<CustomBehavior File="UserSettings" UseMount="False" />
				<RunTo X="190.31" Y="700.34" Z="148.09" />
			</While>
		</While>
	</If>
	
	<If Condition="HasQuest(30518) &amp;&amp; IsQuestCompleted(30518)">
		<CustomBehavior File="FlyTo" X="-257.07" Y="601.18" Z="171.35" />
		<CustomBehavior File="ForcedDismount" />
		<TurnIn QuestName="Farmer Fung's Vote II: On The Loose" QuestId="30518" TurnInName="Farmer Fung" TurnInId="57298" />
		<PickUp QuestName="Farmer Fung's Vote III: Crazy for Cabbage" QuestId="31947" GiverName="Farmer Fung" GiverId="57298" />
	</If>
	
	<!-- Fung's Vote III: Crazy for Cabbage -->
	<If Condition="HasQuest(31947) &amp;&amp; !IsQuestCompleted(31947)" >
		<CustomBehavior File="Message" Text="Starting Crazy for Cabbage" LogColor="Green" />
		<If Condition="IsObjectiveComplete(1,31947)" >
			<CustomBehavior File="Message" Text="Checking for plant." LogColor="Green" />
			<CustomBehavior File="FlyTo" X="-169.0728" Y="638.5255" Z="165.4094" />
			<CustomBehavior File="ForcedDismount" />
			<CustomBehavior File="InteractWith" MobId="58567" NumOfTimes="3" WaitForNpcs="False" />
		</If>
		<If Condition="!HasItem(79102) || (GetItemCount(79102) &lt; 3) &amp;&amp; !IsObjectiveComplete(1,31947)" >
			<CustomBehavior File="Message" Text="Buying Seeds" LogColor="Green" />
			<CustomBehavior File="FlyTo" DestName="Buying Seeds" X="-267.1079" Y="603.3618" Z="167.5482"   />
			<CustomBehavior File="ForcedDismount" />
			<If Condition="GetItemCount(79102) == 0" >
				<CustomBehavior File="InteractWith" MobId="58718" BuyItemId="79102" BuyItemCount="3" X="-268.316" Y="603.4114" Z="167.5478" />
			<ElseIf Condition="GetItemCount(79102) == 1" >
				<CustomBehavior File="InteractWith" MobId="58718" BuyItemId="79102" BuyItemCount="2" X="-268.316" Y="603.4114" Z="167.5478" />
			</ElseIf>
			<ElseIf Condition="GetItemCount(79102) == 2" >
				<CustomBehavior File="InteractWith" MobId="58718" BuyItemId="79102" BuyItemCount="1" X="-268.316" Y="603.4114" Z="167.5478" />
			</ElseIf>
			</If>
			<CustomBehavior File="WaitTimer" WaitTime="2000" />
		</If>
		<While Condition="!IsObjectiveComplete(1,31947)" >
			<CustomBehavior File="FlyTo" DestName="My Farm" X="-168.9621" Y="639.7676" Z="165.4103" />
			<CustomBehavior File="ForcedDismount" />
				<CustomBehavior File="WaitTimer" WaitTime="3000" />
			<CustomBehavior File="InteractWith" MobId="60172" NumOfTimes="1" WaitForNpcs="False"/>
			<CustomBehavior File="InteractWith" MobId="58562" NumOfTimes="1" WaitForNpcs="False"/>
			<CustomBehavior File="UseItemOn" MobId="58563" ItemId="79102" NumOfTimes="1" WaitForNpcs="False" WaitTime="3000"/>
			<!-- Alluring -->
			<CustomBehavior File="Message" Text="Alluring crop..." LogColor="Green" />
			<CustomBehavior File="InteractWith" MobId="60070" NumOfTimes="1" WaitForNpcs="False" WaitTime="1000" />
			<!-- Wiggling -->
			<CustomBehavior File="Message" Text="Wiggling crop..." LogColor="Green" />
			<CustomBehavior File="InteractWith" MobId="60029" NumOfTimes="1" WaitForNpcs="False" WaitTime="1000" />
			<!-- Encroaching Weeds -->
			<CustomBehavior File="Message" Text="Encroaching weeds..." LogColor="Green" />
			<CustomBehavior File="InteractWith" MobId="60185" NumOfTimes="1" WaitForNpcs="False" WaitTime="1000" />
			<If Condition="Me.IsOnTransport" >
				<CustomBehavior File="RunMacro" Macro="/click OverrideActionBarButton1" NumOfTimes="20" WaitTime="250" />
			</If>
			<!-- Tangled -->
			<CustomBehavior File="Message" Text="Tangled crop..." LogColor="Green" />
			<CustomBehavior File="InteractWith" MobId="60270" NumOfTimes="1" WaitForNpcs="False" WaitTime="1000" />
			<If Condition="Me.HasAura(116242)" >
				<RunTo X="-186.6821" Y="642.2603" Z="165.4093" />
				<RunTo X="-168.9621" Y="639.7676" Z="165.4103" />
				<CustomBehavior File="ForcedDismount" />
			</If>
			<!-- Wild -->
			<CustomBehavior File="Message" Text="Wild crop..." LogColor="Green" />
			<CustomBehavior File="InteractWith" MobId="60207" NumOfTimes="1" WaitForNpcs="False" WaitTime="1000" />
			<While Condition="Me.IsOnTransport" >
				<CustomBehavior File="RunMacro" Macro="/click OverrideActionBarButton1" NumOfTimes="40" WaitTime="250" />
			</While>
			<!-- Runty -->
			<CustomBehavior File="Message" Text="Runty crop..." LogColor="Green" />
			<CustomBehavior File="InteractWith" MobId="60218" NumOfTimes="1" WaitForNpcs="False" WaitTime="1500" />
			<CustomBehavior File="Misc\RunLua" Lua="JumpOrAscendStart()" WaitTime="1500" />
		</While>
		
		<!-- If druid cancel shape shift form -->
		<If Condition="Me.Class == WoWClass.Druid || Me.Class == WoWClass.Shaman" >
			<CustomBehavior File="Misc\RunLua" Lua="CancelShapeshiftForm()" />
		</If>
		
		<!-- Use Pest Repeller 1 -->
		<CustomBehavior File="InteractWith" MobId="215163" ObjectType="GameObject" WaitForNpcs="False" WaitTime="1000" X="-154.149" Y="644.668" Z="165.409" />
			
		<!-- Use Sprinkler 1 -->
		<CustomBehavior File="InteractWith" MobId="215137" ObjectType="GameObject" WaitForNpcs="False" WaitTime="1000" X="-154.328" Y="653.969" Z="165.169" />
		
		<!-- Use Pest Repeller 2 -->
		<CustomBehavior File="InteractWith" MobId="215162" ObjectType="GameObject" WaitForNpcs="False" WaitTime="1000" X="-168.795" Y="643.637" Z="165.409" />
			
		<!-- Use Sprinkler 2 -->
		<CustomBehavior File="InteractWith" MobId="215135" ObjectType="GameObject" WaitForNpcs="False" WaitTime="1000" X="-168.587" Y="654.073" Z="165.266" />

		<!-- Tool Time -->
		<If Condition="!HasItem(80513)">
			<CustomBehavior File="InteractWith" MobId="211331" ObjectType="GameObject"  WaitTime="5" X="-166.8677" Y="633.5659" Z="165.4094" />
		</If>
		<If Condition="!HasItem(79104)">
			<CustomBehavior File="InteractWith" MobId="211330" ObjectType="GameObject"  WaitTime="5" X="-166.8677" Y="633.5659" Z="165.4094" />
		</If>
		<!-- Parched -->
		<CustomBehavior File="Message" Text="Watering the plants..." LogColor="Green" />
		<CustomBehavior File="UseItemOn" MobType="Npc" MobId="58565" ItemId="79104" NumOfTimes="16" WaitForNpcs="False" WaitTime="1500"/>
		<!-- Infested -->
		<CustomBehavior File="Message" Text="Spraying the plants..." LogColor="Green" />
		<CustomBehavior File="UseItemOn" MobType="Npc" MobId="60026" ItemId="80513" NumOfTimes="16" WaitForNpcs="False" WaitTime="1500"/>	
		<CustomBehavior File="DeleteItems" Ids="80513,79104"/><!-- Deleting farm tools -->
	</If>
		
	<If Condition="HasQuest(31947) &amp;&amp; IsQuestCompleted(31947)">
		<CustomBehavior File="FlyTo" X="-257.07" Y="601.18" Z="171.35" />
		<CustomBehavior File="ForcedDismount" />
		<TurnIn QuestName="Farmer Fung's Vote III: Crazy For Cabbage" QuestId="31947" TurnInName="Farmer Fung" TurnInId="57298" />
		<CustomBehavior File="Message" Text="Back to farm to finish Farmer Fung's vote." LogColor="Green" />
		<CustomBehavior File="FlyTo" DestName="My Farm" X="-169.7904" Y="647.9799" Z="165.4094" />
	</If>
		
	<CustomBehavior File="LoadProfile" ProfileName="[Rep] Tillers Dailies [Brodie]" />

</QuestOrder>
</HBProfile>