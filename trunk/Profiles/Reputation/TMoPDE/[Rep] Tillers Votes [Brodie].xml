		</While>
	</If>
	<If Condition="HasQuest(31479) &amp;&amp; IsQuestCompleted(31479)">
		<TurnIn QuestName="Way of the Brew" QuestId="31479" TurnInName="Bobo Ironpaw" TurnInId="58717" />
	</If>

<!-- Farmer Fung Quests -->

	<!-- Fung's Vote I: Yak Attack -->
