<!-- Quest Overrides -->
<!-- End of Quest Overrides -->

<QuestOrder>

	<If Condition="Me.IsHorde">
		<CustomBehavior File="LoadProfile" ProfileName="..\[Quest] The Level Grind [Brodie]" />
	</If>
	
	<If Condition="!HasQuest(13594) &amp;&amp; !IsQuestCompleted(13594)" >
		<CustomBehavior File="RunMacro" Macro="/run for i=1,GetNumQuestLogEntries() do SelectQuestLogEntry(i); SetAbandonQuest(); AbandonQuest(); end" />
	</If>

	<CustomBehavior File="Message" Text="Compiling Ashenvale Quests" LogColor="Green" />
	<CustomBehavior File="UserSettings" LootMobs="True" UseFlightPaths="True" PullDistance="20" />
	
	<If Condition="Me.ZoneId == 3524 || Me.ZoneId == 3525" >
		<CustomBehavior File="Message" Text="Running to Exodar's Portal to Darnassus" LogColor="Orange" />
		<RunTo X="-4043.964" Y="-11570.73" Z="-138.4523" />
		<CustomBehavior File="InteractWith" MobId="207996" Range="8" CollectionDistance="1000" X="-4043.964" Y="-11570.73" Z="-138.4523" />
		<CustomBehavior File="WaitTimer" WaitTime="8000" />
	</If>
	
	<If Condition="Me.ZoneId != 331" >
		<If Condition="Me.ZoneId == 1657" >
			<CustomBehavior File="TaxiRide" MobId="40552" DestName="Astranaar" X="9973.32" Y="2623.92" Z="1316.60" />
			<If Condition="Me.ZoneId != 331" >
				<CustomBehavior File="TaxiRide" MobId="40552" DestName="Grove of the Ancients" X="9973.32" Y="2623.92" Z="1316.60" />
			<Else>
				<CustomBehavior File="TaxiRide" MobId="40552" DestName="Darkshore" X="9973.32" Y="2623.92" Z="1316.60" />
			</Else>
			</If>
		</If>
	</If>
	
	<CustomBehavior File="Message" Text="Compiling Orendil's Retreat Quests" LogColor="CornflowerBlue" />
	
	<PickUp QuestName="Don't Forget the Horde" QuestId="13594" GiverName="Sentinel Shyela" GiverId="33187" />
	<PickUp QuestName="Bathran's Hair" QuestId="26473" GiverName="Evenar Stillwhisper" GiverId="33204" />
	<PickUp QuestName="Of Their Own Design" QuestId="13595" GiverName="Bathran" GiverId="33182" />
	<Objective QuestName="Bathran's Hair" QuestId="26473" Type="CollectItem" ItemId="5437" CollectCount="8" />
	<Objective QuestName="Don't Forget the Horde" QuestId="13594" Type="KillMob" MobId="3734" KillCount="10" />
	
	<TurnIn QuestName="Bathran's Hair" QuestId="26473" TurnInName="Evenar Stillwhisper" TurnInId="33204" />
	<TurnIn QuestName="Don't Forget the Horde" QuestId="13594" TurnInName="Sentinel Shyela" TurnInId="33187" />

	<CustomBehavior File="Message" Text="Ashenvale Quests Complete" LogColor="Orange" />
	<CustomBehavior File="LoadProfile" ProfileName="..\[Quest] The Level Grind [Brodie]" />
		
</QuestOrder>
</HBProfile>