<HBProfile>

<Name>Level Checker</Name>

<QuestOrder>

	<If Condition="Me.IsAlliance" >
		<If Condition="Me.Race == WoWRace.Dwarf || Me.Race == WoWRace.Gnome">
			<If Condition="Me.LevelFraction &gt;= 10" >
				<CustomBehavior File="LoadProfile" ProfileName="[Quest] Loch Modan [Brodie]" />
			</If>
			<If Condition="Me.LevelFraction &gt;= 8" >
				<CustomBehavior File="LoadProfile" ProfileName="[Quest] Dun Morogh 2 [Brodie]" />
			</If>
			<If Condition="Me.Race == WoWRace.Dwarf &amp;&amp; Me.LevelFraction &gt;= 5 &amp;&amp; IsQuestCompleted(24493)" >
				<CustomBehavior File="LoadProfile" ProfileName="Vanilla\[Quest] Dun Morogh [Brodie]" />
			</If>
			<If Condition="Me.Race == WoWRace.Gnome &amp;&amp; Me.LevelFraction &gt;= 5 &amp;&amp; IsQuestCompleted(26373)" >
				<CustomBehavior File="LoadProfile" ProfileName="Vanilla\[Quest] Dun Morogh [Brodie]" />
			</If>
		</If>
		<If Condition="Me.Race == WoWRace.NightElf">
			<If Condition="Me.LevelFraction &gt;= 5 &amp;&amp; IsQuestCompleted(28731)" >
				<CustomBehavior File="LoadProfile" ProfileName="Vanilla\[Quest] Teldrassil [Brodie]" />
			</If>
		</If>
	</If>

</QuestOrder>
</HBProfile>